#!/usr/bin/python3
import pytz,datetime
datetime.datetime.now(pytz.utc).isoformat()
sudo btrfs subvolume snapshot -r /disks/music /tmp/stuart2Tb/snapshots/music/2013-03-04T00:33:24.009229+00:00
